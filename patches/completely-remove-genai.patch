-    Glean.genaiChatbot.providerChange.record({
-      current: providerId,
-      previous: renderProviders.lastId,
-      surface: "panel",
-    });
-  }
-  renderProviders.lastId = providerId;
-
-  // Load the requested provider
-  request();
-  return select;
-}
-
